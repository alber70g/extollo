  },
  "author": "Albert Groothedde <albertgroothedde@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/tape": "^4.2.32",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.1.0",
    "redux": "^4.0.1",
    "tape": "^4.9.1",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.2"
  },
  "dependencies": {},
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.test.ts"
    ]
